alias gs='git status'
alias ga='git add'
alias gb='git branch'
alias gc='git commit'
alias gd='git diff'
alias gco='git checkout'
alias gf='git fetch --prune'
alias gm='git merge'
alias gl='git log'
alias gt='git log --graph --oneline'
alias gta='git log --graph --oneline --all'
alias gp='git push'
alias gh='git log --pretty=format:"%h [%an] %ad | %s%d" --graph --date=short'
